{
  "manifest_version": 2,
  "name": "Wikipedia MonoBook Skin Redirect",
  "version": "1.1",
  "description": "Redirects Wikipedia articles to the MonoBook theme version of the page. Forked from https://addons.mozilla.org/en-US/firefox/addon/wikipedia-vector-skin/",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*.mediawiki.org/*",
    "*://*.wikipedia.org/*",
    "*://*.wiktionary.org/*",
    "*://*.wikiquote.org/*",
    "*://*.wikiversity.org/*",
    "*://*.wikivoyage.org/*",
    "*://*.wikimedia.org/*",
    "*://*.wikidata.org/*",
    "*://*.wikinews.org/*",
    "*://*.wikisource.org/*",
    "*://*.wikibooks.org/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{326a497d-1470-4b0e-b320-d10387c4f85d}"
    }
  }
}